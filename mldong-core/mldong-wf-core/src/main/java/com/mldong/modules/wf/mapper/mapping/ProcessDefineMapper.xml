<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mldong.modules.wf.mapper.ProcessDefineMapper">

    <select id="selectCustom" resultType="com.mldong.modules.wf.vo.ProcessDefineVO">
        select * from wf_process_define t ${ew.customSqlSegment}
    </select>
    <select id="findById" resultType="com.mldong.modules.wf.vo.ProcessDefineVO">
        select * from wf_process_define t where t.id = #{id}
    </select>
    <select id="selectLastByName" resultType="com.mldong.modules.wf.vo.ProcessDefineVO">
        select * from wf_process_define t where t.name = #{name} and t.state=1 order by version desc limit 1
    </select>
</mapper>
