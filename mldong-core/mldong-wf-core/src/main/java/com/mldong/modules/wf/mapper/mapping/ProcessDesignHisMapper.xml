<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mldong.modules.wf.mapper.ProcessDesignHisMapper">

    <select id="selectCustom" resultType="com.mldong.modules.wf.vo.ProcessDesignHisVO">
        select * from wf_process_design_his t ${ew.customSqlSegment}
    </select>
    <select id="findById" resultType="com.mldong.modules.wf.vo.ProcessDesignHisVO">
        select * from wf_process_design_his t where t.id = #{id}
    </select>
    <select id="getLatestByProcessDesignId" resultType="com.mldong.modules.wf.entity.ProcessDesignHis">
        select * from wf_process_design_his where process_design_id=#{processDesignId} order by id desc limit 1
    </select>
</mapper>
